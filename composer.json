{
    "name": "raxos/search",
    "description": "Search provider for Raxos. Uses raxos/database.",
    "license": "mit",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "type": "library",
    "authors": [
        {
            "name": "Bas Milius",
            "email": "bas@mili.us"
        }
    ],
    "autoload": {
        "psr-4": {
            "Raxos\\Search\\": "src/"
        }
    },
    "config": {
        "platform": {
            "php": "8.4"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../Projects/raxos/*",
            "symlink": true
        }
    ],
    "require": {
        "ext-ctype": "*",
        "ext-mbstring": "*",
        "jetbrains/phpstorm-attributes": "^1.2",
        "raxos/database": "*",
        "raxos/datetime": "*",
        "raxos/foundation": "*"
    }
}
